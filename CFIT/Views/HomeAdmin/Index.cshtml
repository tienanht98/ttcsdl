@model IEnumerable<CFIT.EF.TTCSDL_GiaoVien>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "/Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HoTen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DonVi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CapBac)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HoTen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DonVi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CapBac)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDGiaoVien }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDGiaoVien }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDGiaoVien })
        </td>
    </tr>
}

</table>
